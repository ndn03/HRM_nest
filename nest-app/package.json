{
  "name": "nest-base",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "keywords": [],
  "engines": {
    "node": "20.12.2"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "console:dev": "ts-node -r tsconfig-paths/register src/console.ts",
    "console": "node dist/console.js",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d ./dist/configs/typeorm.config.js",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate ./src/migrations/$NAME -d ./dist/configs/typeorm.config.js",
    "migration:create": "npm run typeorm migration:create ./src/migrations/$NAME -d ./dist/configs/typeorm.config.js",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d ./dist/configs/typeorm.config.js",
    "migration:show": "typeorm-ts-node-commonjs migration:show -d ./dist/configs/typeorm.config.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/lib-storage": "^3.705.0",
    "@fastify/compress": "^7.0.0",
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.0",
    "@fastify/csrf-protection": "^6.4.0",
    "@fastify/helmet": "^11.0.0",
    "@fastify/multipart": "^8.3.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/static": "^7.0.4",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^3.1.2",
    "@nestjs/bullmq": "^10.2.2",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-fastify": "^10.4.9",
    "@nestjs/schedule": "^4.1.1",
    "@nestjs/swagger": "^8.0.7",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/typeorm": "^10.0.2",
    "axios": "^1.7.8",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.29.1",
    "cache-manager": "^5.2.4",
    "cache-manager-redis-yet": "^5.1.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "fastify-multer": "^2.0.3",
    "handlebars": "^4.7.8",
    "ioredis": "^5.4.1",
    "mysql2": "^3.11.4",
    "nestjs-console": "^8.0.0",
    "nestjs-i18n": "^10.5.0",
    "nestjs-typeorm-paginate": "^4.0.4",
    "nodemailer": "^6.9.16",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@src/(.*)$": "<rootDir>/$1",
      "^@commands/(.*)$": "<rootDir>/commands/$1",
      "^@common/(.*)$": "<rootDir>/common/$1",
      "^@configs/(.*)$": "<rootDir>/configs/$1",
      "^@entities/(.*)$": "<rootDir>/entities/$1",
      "^@languages/(.*)$": "<rootDir>/languages/$1",
      "^@filters/(.*)$": "<rootDir>/filters/$1",
      "^@guards/(.*)$": "<rootDir>/guards/$1",
      "^@interceptors/(.*)$": "<rootDir>/interceptors/$1",
      "^@modules/(.*)$": "<rootDir>/modules/$1"
    }
  }
}
